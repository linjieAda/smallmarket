{
  "extends": "airbnb",
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
  },
  "parser": "babel-eslint",
  "rules": {
    "semi": 0,
    "import/default": 0,
    "import/no-duplicates": 0,
    "import/named": 0,
    "import/namespace": 0,
    "import/no-unresolved": 0,
    "import/no-named-as-default": 2,
    "comma-dangle": 0,
    "no-console": 0,
    "no-alert": 0,
    "react/prop-types": 0,
    "max-len": [1, 150, 4],
    "no-multiple-empty-lines": [1, {max: 2}],
    "global-require": 0,
  }
}
